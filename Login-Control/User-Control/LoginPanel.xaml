<UserControl x:Class="User_Control.LoginPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:User_Control"
             mc:Ignorable="d" 
             d:DesignHeight="160" d:DesignWidth="300">
    <Grid>

            <TextBox x:Name="tbUsuario" Margin="87,48,10,89" TextWrapping="Wrap"/>
            <TextBox x:Name="tbContrasenya" Height="23" Margin="87,89,10,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
            <Label x:Name="lblUsuario" Content="Usuario:" Margin="29,45,218,89"/>
            <Label x:Name="lblTitulo" Content="Inicio sesión" Margin="120,10,105,0" Height="26" VerticalAlignment="Top"/>
            <Label x:Name="lblContrasenya" Content="Contraseña:" Margin="10,86,0,48" HorizontalAlignment="Left" Width="72"/>
            <Button x:Name="btnCancelar" Content="Cancelar" Margin="215,131,10,10"/>
            <Button x:Name="btnLogin" Content="Iniciar sesión" Margin="135,131,90,10" Click="onClickLogin"/>
      
    </Grid>
</UserControl>
